{
  "title": "Gyazo Teams settings",
  "rules": [
    {
      "description": "⌘+⇧ +1でGyazo Teams起動",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "simultaneous": [
              { "key_code": "1" }
            ],
            "modifiers": {
              "mandatory": ["command", "shift"],
              "optional": [ "any" ]
            }
          },
          "to": [
            { "shell_command": "open -a 'Gyazo Teams.app'" }
          ]
        }
      ]
    },
